@model Registeration

<table border="1" align="center">
    <tr>
        <td> Full Name </td>
        <td>@Model.fName &nbsp; @Model.mName &nbsp;@Model.lName</td>
    </tr>
    <tr>
        <td>Date of Birth </td>
        <td>@Model.Date &nbsp; @Model.Month &nbsp; @Model.Year</td>
    </tr>
    <tr>
        <td>Street Address</td>
        <td>@Model.StreetAddress</td>
    </tr>
    <tr>
        <td>Street Address1</td>
        <td>@Model.StreetAddress1</td>
    </tr>
    <tr>
        <td>City</td>
        <td>@Model.StreetAddress1</td>
    </tr>
    <tr>
        <td>State / Province</td>
        <td>@Model.State</td>
    </tr>
    <tr>
        <td>Postal/Zip Code</td>
        <td>@Model.PinCode</td>
    </tr>
</table>